{
    "name": "Tabs Menu",
    "version": "0.1.0",
    "manifest_version": 2,
    "author": "run little sun",
    "homepage_url": "https://github.com/runlittlesun",
    "description": "too many tabs",
    "icons": {
        "16": "images/icon16.png",             
        "48": "images/icon48.png",            
        "128": "images/icon128.png"
    },
    "browser_action":{
        "default_icon": "images/icon.png",
        "default_title": "TabsMenu",
        "default_popup": "html/popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab"
    ]
}